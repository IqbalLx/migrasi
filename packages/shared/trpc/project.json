{
  "name": "shared-trpc",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/shared/trpc/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/shared/trpc",
        "main": "packages/shared/trpc/src/index.ts",
        "tsConfig": "packages/shared/trpc/tsconfig.lib.json",
        "assets": ["packages/shared/trpc/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/shared/trpc/**/*.ts"]
      }
    }
  },
  "tags": []
}
